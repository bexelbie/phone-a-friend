{
  "name": "@bexelbie/phone-a-friend",
  "version": "0.1.0",
  "description": "MCP server that lets VS Code Copilot Chat invoke Copilot CLI with a different model",
  "repository": {
    "type": "git",
    "url": "https://github.com/bexelbie/phone-a-friend"
  },
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "phone-a-friend": "dist/index.js"
  },
  "files": [
    "dist/",
    "!dist/test/"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsc --watch",
    "test": "node --test dist/test/*.test.js",
    "pretest": "tsc"
  },
  "keywords": [
    "mcp",
    "copilot",
    "github-copilot",
    "multi-model",
    "subagent"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=22.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.26.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.7.0"
  }
}
